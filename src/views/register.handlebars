<div class="container m-5">
    <form id="registerForm">
        <div class="form-group m-4">
            <label class="font-weight-bold">Nombre</label>
            <input class="form-control" name="first_name">
        </div>
        <div class="form-group m-4">
            <label class="font-weight-bold">Apellido</label>
            <input class="form-control" name="last_name">
        </div>
        <div class="form-group m-4">
            <label class="font-weight-bold">Email</label>
            <input class="form-control" name="email">
        </div>
        <div class="form-group m-4">
            <label class="font-weight-bold">Telefono</label>
            <input class="form-control" name="telefono">
        </div>
        <div class="form-group m-4">
            <label class="font-weight-bold">Contraseña</label>
            <input class="form-control" type="password" name="password">
        </div>
        <div class="form-group m-4">
            <label class="font-weight-bold">Repetir contraseña</label>
            <input class="form-control" type="password" name="re-password">
        </div>
        <div class="form-group m-4">
            <input class="btn btn-primary rounded mb-4 mll-4 px-3 py-2" type="submit">
        </div>
    </form>
</div>
<div class="container m-5">
    <a href="/login"><button class="btn btn-primary rounded mb-4 mll-4 px-3 py-2">Ir a Login</button></a>
</div>

<script src="js/register.js"></script>